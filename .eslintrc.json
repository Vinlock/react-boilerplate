{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true,
      "modules":true,
      "arrowFunctions":true,
      "classes":true,
      "spread":true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": "airbnb",
  "rules": {
    "func-names": ["error", "never"],
    "no-console": 0
  }
}